{
  "name": "@codecorn/empty-session-reaper",
  "version": "1.0.0",
  "description": "Clean up & inspect zombie sessions in Express: prunes cookie-only sessions to keep Prisma/Redis stores lean.",
  "keywords": [
    "express",
    "session",
    "middleware",
    "cleanup",
    "redis",
    "prisma",
    "connect-flash",
    "passport"
  ],
  "license": "ISC",
  "author": "CodeCorn fgirolami29 <f.girolami@codecorn.it>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodeCornTech/empty-session-reaper.git"
  },
  "bugs": {
    "url": "https://github.com/CodeCornTech/empty-session-reaper/issues"
  },
  "homepage": "https://github.com/CodeCornTech/empty-session-reaper#readme",
  "type": "commonjs",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./presets": "./dist/presets.js",
    "./express": "./dist/express.js"
  },
  "typesVersions": {
    "*": {
      "presets": [
        "dist/presets.d.ts"
      ],
      "express": [
        "dist/express.d.ts"
      ],
      "*": [
        "dist/index.d.ts"
      ]
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": false,
  "engines": {
    "node": ">=16"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "npm run clean && tsc -p tsconfig.json",
    "check": "tsc -p tsconfig.json --noEmit",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "rimraf": "^5.0.7",
    "typescript": "^5.5.4"
  }
}
